<?xml version="1.0" encoding="UTF-8"?>
<hazelcast
        xsi:schemaLocation="http://www.hazelcast.com/schema/config http://www.hazelcast.com/schema/config/hazelcast-config.xsd"
        xmlns="http://www.hazelcast.com/schema/config"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <management-center enabled="false">http://192.168.0.37:8080/hazelcast-mancenter</management-center>

    <network>
        <join>
            <multicast enabled="false">
            </multicast>
            <tcp-ip enabled="true">
                <member>192.168.0.16</member>
            </tcp-ip>
        </join>
    </network>

    <properties>
        <property name="hazelcast.rest.enabled">true</property>
    </properties>

</hazelcast>